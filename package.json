    "@dcloudio/uni-quickapp-native": "^2.0.0-28520200811001",
    "@dcloudio/uni-quickapp-webview": "^2.0.0-28520200811001",
    "@dcloudio/uni-stat": "^2.0.0-28520200811001",
    "@vue/shared": "^3.0.0-rc.4",
    "core-js": "^3.6.5",
    "flyio": "^0.6.2",
