    "@dcloudio/vue-cli-plugin-hbuilderx": "^2.0.0-28520200811001",
    "@dcloudio/vue-cli-plugin-uni": "^2.0.0-28520200811001",
    "@dcloudio/vue-cli-plugin-uni-optimize": "^2.0.0-28520200811001",
    "@dcloudio/webpack-uni-mp-loader": "^2.0.0-28520200811001",
    "@dcloudio/webpack-uni-pages-loader": "^2.0.0-28520200811001",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-plugin-import": "^1.11.0",
    "cross-env": "^7.0.2",
    "jest": "^25.4.0",
    "mini-types": "*",
    "miniprogram-api-typings": "*",
    "postcss-comment": "^2.0.0",
    "vue-template-compiler": "^2.6.11",
    "babel-eslint": "10.0.1",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "5.2.2"
  },
  "browserslist": [
    "Android >= 4",
    "ios >= 8"
  ],
  "uni-app": {
    "scripts": {}
  }
}
